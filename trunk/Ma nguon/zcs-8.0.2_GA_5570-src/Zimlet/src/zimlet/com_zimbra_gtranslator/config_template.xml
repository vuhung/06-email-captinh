       <property name="GOOGLE_TRANSLATE_API_KEY">asdfsd6666sdfsd6fsdf6sd</property>
       For more info:
      http://code.google.com/apis/language/translate/v2/pricing.html -->
    <property name="GOOGLE_TRANSLATE_API_KEY"></property>
  </global>
</zimletConfig>
