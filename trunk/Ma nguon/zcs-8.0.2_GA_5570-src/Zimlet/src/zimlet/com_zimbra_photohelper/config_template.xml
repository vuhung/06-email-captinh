<zimletConfig name="com_zimbra_photohelper" version="0.1">
    <global>
        <!-- photoLocatorURI Enter absolute url to the photo's url and replace string that points to photo name with "{photoFileName}".
            For example: http://www.mycompany.com/people?name=rrao to
             http://www.mycompany.com/people?name={photoFileName}

           PS: The Web client will replace back {photoFileName} with real-photo name when it constructs the url.
         -->
        <property name="photoLocatorURI"></property>

        <!--  Set this to true, if the photo url should use Zimbra's proxy servlet -->
        <property name="useProxy">false</property>
    </global>
</zimletConfig>